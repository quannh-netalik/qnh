{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": ["javascript"],
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
  },
  "search.exclude": {
    "**/node_modules": true,
    "package-lock.json": true,
    "dist": true,
    "build": true
  }
}
